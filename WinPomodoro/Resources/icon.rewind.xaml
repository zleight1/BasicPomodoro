
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DrawingBrush x:Key="BrushRewind" Stretch="Uniform">
        <DrawingBrush.Drawing>
            <DrawingGroup>
                <DrawingGroup.Children>
                    <GeometryDrawing Brush="#FF000000" Geometry="F1 M 36,17.5L 27.5,24L 36,30.5L 36,17.5 Z " />
                    <GeometryDrawing Brush="#FF000000" Geometry="F1 M 25,17.5L 16.5,24L 25,30.5L 25,17.5 Z " />
                    <GeometryDrawing Brush="#FF000000" Geometry="F1 M 15,18L 12,18L 12,30L 15,30L 15,18 Z " />
                </DrawingGroup.Children>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>

    <Rectangle x:Key="IconRewind"
               Width="16"
               Height="16"
               Fill="{StaticResource BrushRewind}"
               x:Shared="False" />
</ResourceDictionary>
